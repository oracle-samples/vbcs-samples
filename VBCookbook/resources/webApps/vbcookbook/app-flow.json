{
  "id": "vbcookbook",
  "description": "VB Cookbook",
  "defaultPage": "shell",
  "chains": {},
  "services": {},
  "configuration": {
    "initParams": {
      "ojSpVersion": "{{window.appConfig && window.appConfig['OJ_SP_VERSION'] ? window.appConfig['OJ_SP_VERSION'] : '2307.0.1'}}",
      "cdnLocation": "{{window.FAEndPoints && window.FAEndPoints['CDN_LOCATION'] ? window.FAEndPoints['CDN_LOCATION'] : 'https://static.oracle.com/cdn/'}}",
      "ojSpDebug": "{{window.appConfig && window.appConfig.DEBUG ? window.appConfig.DEBUG : false}}"
    },
    "profile": "DT"
  },
  "translations": {
    "app": {
      "path": "./resources/strings/app/nls/app-strings"
    },
    "appBundle": {
      "path": "./resources/strings/app/nls/app-strings"
    }
  },
  "events": {
    "navigateToItem": {
      "payloadType": {
        "item": "string"
      }
    }
  },
  "types": {
    "DocReferenceType": {
      "label": "string",
      "type": "string",
      "url": "string"
    },
    "RecipeType": {
      "apiRef": "DocReferenceType[]",
      "category": "string",
      "desc": "string",
      "id": "string",
      "label": "string",
      "icon": "string",
      "redwood": "string",
      "seeAlso": "DocReferenceType[]",
      "shortDesc": "string",
      "subCategory": "string"
    }
  },
  "variables": {
    "canNavigateBack": {
      "type": "boolean",
      "defaultValue": false
    },
    "currentRecipe": {
      "type": "object",
      "defaultValue": "[[ $application.functions.getRecipeMetadataForPage($application.currentPage )]]"
    },
    "isMobileDevice": {
      "type": "boolean",
      "defaultValue": "{{ $application.functions.isMobileDevice() }}"
    },
    "isThisVBDesigner": {
      "type": "boolean",
      "defaultValue": "{{ window.vbInitConfig.RUNTIME_ENV_CLASS === 'vbdt/preview-core/rt/DtRuntimeEnvironment' || $application.variables.hideRecipe }}",
      "description": "This is helper variable which is used to hide some content when VB Cookbook is opened in VB Design Time. The purpose is to streamline working with individual recipes."
    },
    "hideRecipe": {
      "type": "boolean",
      "defaultValue": false,
      "input": "fromUrl",
      "description": "This can be set to true on URL to force VB Cookbook to behave as if it was opened in VB Designer, that is rendering without recipes subflow. It is used from functional tests execution as vb-webdriver Expectation has issues with testing pages with subflow"
    },
    "navMenuVisible": {
      "type": "boolean",
      "defaultValue": true
    }
  },
  "userConfig": {
    "type": "vb/DefaultSecurityProvider",
    "configuration": {},
    "embedding": "deny"
  },
  "security": {
    "access": {
      "requiresAuthentication": false
    }
  },
  "localization": {
    "locale": "{{ window.localStorage.getItem('vbcs.languageSwitcherApplication.locale') || 'en' }}"
  },
  "requirejs": {
    "paths": {
      "colorthief": "https://static.oracle.com/cdn/spectra-ui/oj-sp/2307.0.1/3rdparty/color-thief_2.3.2/dist",
