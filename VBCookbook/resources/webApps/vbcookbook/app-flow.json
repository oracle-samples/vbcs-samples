      "fndconfig": "{{($initParams.ojSpDebug ? $initParams.cdnLocation + 'spectra-ui/oj-sp/' + $initParams.ojSpVersion + '/debug' : $initParams.cdnLocation + 'spectra-ui/oj-sp/' + $initParams.ojSpVersion) + '/spectra-shell/config'}}",
