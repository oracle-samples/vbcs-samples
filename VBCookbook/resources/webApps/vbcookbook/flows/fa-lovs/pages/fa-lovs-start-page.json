{

  "title": "",
  "description": "",
  "variables": {
    "employeeListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Employee",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "page:getallEmployeeResponse2",
        "transformsContext": {
          "vb-textFilterAttributes": [
            "firstName"
          ]
        }
      }
    },
    "selectedEmployee": {
      "type": "object"
    },
    "selectedRow": {
      "type": {
        "key": "string"
      }
    },
    "filters": {
      "type": "filter[]",
      "defaultValue": [
        {
          "op": "$sw",
          "attribute": "firstName",
          "value": ""
        },
        {
          "op": "$sw",
          "attribute": "lastName",
          "value": ""
        },
        {
          "op": "$eq",
          "attribute": "departmentObject.id",
          "value": null
        },
        {
          "op": "$eq",
          "attribute": "skillCollection.id",
          "value": []
        },
        {
          "op": "$ge",
          "attribute": "salary",
          "value": null
        },
        {
          "op": "$le",
          "attribute": "salary",
          "value": null
        }
      ]
    },
    "departmentListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Department",
        "idAttribute": "id",
        "itemsPath": "items",
        "responseType": "page:getallDepartmentResponse",
        "transformsContext": {
          "vb-textFilterAttributes": [
            "departmentName"
          ]
        }
      }
    },
    "skillListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Skill",
        "idAttribute": "id",
        "itemsPath": "items",
        "responseType": "page:getallSkillResponse"
      }
    },
    "AdvanceSearchEmployeeListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Employee",
        "idAttribute": "id",
        "itemsPath": "items",
        "responseType": "page:getallEmployeeResponseForAdvanceSearch"
      }
    }
  },
  "metadata": {},
  "types": {
    "@decorators.getallEmployeeResponse2": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "filter": {
      "op": "string",
      "attribute": "string",
      "value": "any"
    },
    "getallDepartmentResponse": {
      "items": [
        {
          "departmentName": "string",
          "id": "number"
        }
      ]
    },
    "@decorators.getallDepartmentResponse": {
      "endpoint": "getall_Department",
      "service": "businessObjects",
      "path": "response"
    },
    "getallEmployeeResponse2": {
      "items": [
        {
          "firstName": "string",
          "id": "number",
          "lastName": "string"
        }
      ]
    },
    "getallSkillResponse": {
      "items": [
        {
          "id": "number",
          "name": "string"
        }
      ]
    },
    "@decorators.getallSkillResponse": {
      "endpoint": "getall_Skill",
      "service": "businessObjects",
      "path": "response"
    },
    "getallEmployeeResponseForAdvanceSearch": {
      "items": [
        {
          "departmentObject": {
            "items": [
              {
                "departmentName": "string"
              }
            ]
          },
          "firstName": "string",
          "id": "number",
          "jobObject": {
            "items": [
              {
                "jobTitle": "string"
              }
            ]
          },
          "lastName": "string",
          "salary": "number",
          "skillCollection": {
            "items": [
              {
                "name": "string"
              }
            ]
          }
        }
      ]
    },
    "@decorators.getallEmployeeResponseForAdvanceSearch": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    }
  },
  "chains": {
    "OpenAdvancedSearchDialog": {
      "description": "",
      "variables": {
        "detail": {
          "required": true,
          "type": "any",
          "input": "fromCaller"
        }
      },
      "root": "openDialogComponentMethod",
      "actions": {
        "openDialogComponentMethod": {
          "module": "vb/action/builtin/callComponentMethodAction",
          "parameters": {
            "component": "{{ document.getElementById('advanced-search-oj-dialog') }}",
            "method": "open"
          }
        }
      }
    },
    "OKButtonActionChain": {
      "description": "",
      "variables": {
        "detail": {
          "required": true,
          "type": "any",
          "input": "fromCaller"
        }
      },
      "root": "if1",
      "actions": {
        "closeDialogComponentMethod": {
          "module": "vb/action/builtin/callComponentMethodAction",
          "parameters": {
            "component": "{{ document.getElementById('advanced-search-oj-dialog') }}",
            "method": "close"
          },
          "outcomes": {
            "success": "resetVariables1"
          }
        },
        "if1": {
          "module": "vb/action/builtin/ifAction",
          "parameters": {
            "condition": "[[ $page.variables.selectedRow.key ]]"
          },
          "outcomes": {
            "true": "assignVariables1",
            "false": "closeDialogComponentMethod"
          }
        },
        "assignVariables1": {
          "module": "vb/action/builtin/assignVariablesAction",
          "parameters": {
            "$page.variables.selectedEmployee": {
              "source": {
                "data": {
                  "firstName": "{{$page.variables.selectedRow.data.firstName}}",
                  "id": "{{$page.variables.selectedRow.key}}",
                  "lastName": "{{$page.variables.selectedRow.data.lastName}}"
                },
                "metadata": {
                  "key": "{{$page.variables.selectedRow.key}}"
                },
                "key": "{{$page.variables.selectedRow.key}}"
              },
              "auto": "always",
              "reset": "none"
            }
          },
          "outcomes": {
            "success": "closeDialogComponentMethod"
          }
        },
        "resetVariables1": {
          "module": "vb/action/builtin/resetVariablesAction",
          "parameters": {
            "variables": [
              "$page.variables.filters",
              "$page.variables.AdvanceSearchEmployeeListSDP.filterCriterion",
              "$page.variables.selectedRow"
            ]
          }
        }
      }
    },
    "CancelButtonActionChain": {
      "description": "",
      "variables": {
        "detail": {
          "required": true,
          "type": "any",
          "input": "fromCaller"
        }
      },
      "root": "resetVariables1",
      "actions": {
        "CancelButtonActionChain": {
          "module": "vb/action/builtin/callComponentMethodAction",
          "parameters": {
            "component": "{{ document.getElementById('advanced-search-oj-dialog') }}",
            "method": "close"
          }
        },
        "resetVariables1": {
          "module": "vb/action/builtin/resetVariablesAction",
          "parameters": {
            "variables": [
              "$page.variables.selectedRow",
              "$page.variables.filters",
              "$page.variables.AdvanceSearchEmployeeListSDP.filterCriterion"
            ]
          },
          "outcomes": {
            "success": "CancelButtonActionChain"
          }
        }
      }
    },
    "ResetFiltersActionChain": {
      "description": "",
      "variables": {
        "detail": {
          "required": true,
          "type": "any",
          "input": "fromCaller"
        }
      },
      "root": "FilterResetVariables",
      "actions": {
        "FilterResetVariables": {
          "module": "vb/action/builtin/resetVariablesAction",
          "parameters": {
            "variables": [
              "$page.variables.AdvanceSearchEmployeeListSDP.filterCriterion",
              "$page.variables.filters",
              "$page.variables.departmentListSDP.filterCriterion.criteria"
            ]
          }
        }
      }
    },
    "ApplyFilterActionChain": {
      "description": "",
      "variables": {
        "detail": {
          "required": true,
          "type": "any",
          "input": "fromCaller"
        }
      },
      "root": "filterModuleFunction",
      "actions": {
        "filterModuleFunction": {
          "module": "vb/action/builtin/callModuleFunctionAction",
          "parameters": {
            "module": "[[ $page.functions ]]",
            "functionName": "mapToCriteria",
            "params": [
              "{{ $page.variables.filters }}"
            ]
          },
          "outcomes": {
            "success": "filtercriterionAssignVariable"
          }
        },
        "filtercriterionAssignVariable": {
          "module": "vb/action/builtin/assignVariablesAction",
          "parameters": {
            "$page.variables.AdvanceSearchEmployeeListSDP": {
              "source": {
                "filterCriterion": {
                  "op": "$and",
                  "criteria": "{{ $chain.results.filterModuleFunction }}"
                }
              },
              "reset": "none",
              "auto": "always",
              "mapping": {
                "$target.filterCriterion": {
                  "source": "$source.filterCriterion",
                  "reset": "none",
                  "auto": "always",
                  "mapping": {
                    "$target.criteria": {
                      "source": "$source.criteria",
                      "reset": "empty"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "DeSelectEmployeeChain": {
      "description": "",
      "variables": {
        "detail": {
          "required": true,
          "type": "any",
          "input": "fromCaller"
        }
      },
      "root": "resetselectedEmployee",
      "actions": {
        "resetselectedEmployee": {
          "module": "vb/action/builtin/resetVariablesAction",
          "parameters": {
            "variables": [
              "$page.variables.selectedEmployee"
            ]
          }
        }
      }
    }
  },
  "eventListeners": {
    "employeeAdvSearch": {
      "chains": [
        {
          "chainId": "OpenAdvancedSearchDialog",
          "parameters": {
            "detail": "{{ $event.detail }}"
          }
        }
      ]
    },
    "spanClick": {
      "chains": [
        {
          "chainId": "DeSelectEmployeeChain",
          "parameters": {
            "detail": "{{ $event.detail }}"
          }
        }
      ]
    },
    "okojButtonOjAction": {
      "chains": [
        {
          "chainId": "OKButtonActionChain"
        }
      ]
    },
    "searchojButtonOjAction": {
      "chains": [
        {
          "chainId": "ApplyFilterActionChain",
          "parameters": {
            "detail": "{{ $event.detail }}"
          }
        }
      ]
    },
    "resetojButtonOjAction": {
      "chains": [
        {
          "chainId": "ResetFiltersActionChain",
          "parameters": {
            "detail": "{{ $event.detail }}"
          }
        }
      ]
    },
    "cancelojButtonOjAction": {
      "chains": [
        {
          "chainId": "CancelButtonActionChain"
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-label": {
        "path": "ojs/ojlabel"
      },
      "oj-dialog": {
        "path": "ojs/ojdialog"
      },
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-table": {
        "path": "ojs/ojtable"
      },
      "oj-radioset": {
        "path": "ojs/ojradioset"
      },
      "oj-option": {
        "path": "ojs/ojoption"
      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-select-single": {
        "path": "ojs/ojselectsingle"
      },
      "oj-select-many": {
        "path": "ojs/ojselectmany"
      },
      "oj-input-number": {
        "path": "ojs/ojinputnumber"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      }
    }
  }
}